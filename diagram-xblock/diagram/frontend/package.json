{
    "name": "diagram-js",
    "type": "module",
    "scripts": {
        "build": "rollup -c rollup.config.js --input src/diagram.tsx && rollup -c rollup.config.js --input src/diagram_studio.tsx",
        "prebuild": "npm run i18n:compile",
        "watch": "rollup -c rollup.config.js --input src/diagram.tsx --watch",
        "watch_studio": "rollup -c rollup.config.js --input src/diagram_studio.tsx --watch",
        "prewatch": "npm run i18n:compile",
        "i18n:extract": "formatjs extract src/**/*.ts* --ignore='**/*.d.ts' --out-file lang/en.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
        "i18n:compile": "formatjs compile-folder --ast ./lang/ ./lang/compiled/"
    },
    "dependencies": {
        "@radix-ui/react-slot": "^1.2.4",
        "@rollup/plugin-image": "^3.0.3",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "fabric": "^5.5.2",
        "html2canvas": "^1.4.1",
        "install": "^0.13.0",
        "jsxgraph": "^1.12.2",
        "lodash": "^4.17.21",
        "lodash-es": "^4.17.21",
        "lucide-react": "^0.553.0",
        "postcss": "^8.5.6",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-router-dom": "^6.28.0",
        "rollup-plugin-postcss": "^4.0.2",
        "tailwind-merge": "^3.4.0"
    },
    "devDependencies": {
        "@formatjs/cli": "^6.3.11",
        "@formatjs/ts-transformer": "^3.13.23",
        "@rollup/plugin-commonjs": "^28.0.1",
        "@rollup/plugin-json": "^6.1.0",
        "@rollup/plugin-node-resolve": "^15.3.0",
        "@rollup/plugin-replace": "^6.0.1",
        "@rollup/plugin-typescript": "^12.1.1",
        "@rollup/plugin-url": "^8.0.2",
        "@svgr/rollup": "^6.2.1",
        "@tailwindcss/postcss": "^4.1.18",
        "@types/fabric": "^3.6.2",
        "@types/file-saver": "^2.0.7",
        "@types/hoist-non-react-statics": "^3.3.1",
        "@types/jest": "^24.0.0",
        "@types/lodash": "^4.14.161",
        "@types/node": "^20",
        "@types/react": "^18.3.12",
        "@types/react-dom": "^18.3.1",
        "@vitejs/plugin-react": "^4.3.4",
        "autoprefixer": "^10.4.23",
        "eslint": "^9",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-intl": "^7.0.1",
        "rollup": "^4.28.0",
        "tailwindcss": "^4.1.18",
        "tailwindcss-animate": "^1.0.7",
        "tw-animate-css": "^1.4.0",
        "typescript": "5.7.2",
        "vite": "^6.0.1"
    }
}
